// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(cuid())
  phone       String
  requests    Requests[]
}

model Requests {
  id          String @id @default(cuid())
  ccde        String
  itinerary   String
  users       User[]
}